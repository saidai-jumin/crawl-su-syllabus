{
  "name": "bun-ex",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./ && npm run dts-esm && npm run dts-cjs",
    "dts-esm": "tsup ./src/index.ts --dts-only --format esm --out-dir ./",
    "dts-cjs": "tsup ./src/index.ts --dts-only --format cjs --out-dir ./"

  },
  "files": [
    "**/*.ts",
    "./dist/index.d.ts",
    "tsconfig.json",
    "!**/*.test.ts",
    "!**/__test__"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12"
  }
}