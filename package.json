{
  "name": "bun-ex",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:ts": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:cjs": "tsc-alias --project tsconfig.cjs.json",
    "build:esm": "tsc-alias --project tsconfig.esm.json",
    "build": "npm run build:ts && npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12"
  }
}
